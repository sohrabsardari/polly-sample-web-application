{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Polly": {
    "CircuitBreaker": {
      "FailureThreshold": 0.5,
      "SamplingDurationInSecond": 10,
      "MinimumThroughput": 8,
      "DurationOfBreakInSecond": 12
    },
    "Timeout": {
      "TimeoutInSecond": 12
    },
    "BulkHead": {
      "MaxParallelization": 10,
      "MaxQueuingActions": 50
    }
  }
}
